.contain{
    width: 100vw;
    background-color: #EDEDED;
}
#head{
    width: 100vw;
    height: 25vh;
    display: flex;
    align-items: center; 
    justify-content: center;
    box-shadow: 0rpx 2rpx 10rpx 10rpx rgb(151, 149, 149);
    position: relative;
}
.back{
    width: 100%;
    height: 100%;
}
#head .imag{
    position: absolute;
    width: 120rpx;
    height: 120rpx;
    border-radius: 50%;
    top: 50%;
    left: 50%;
    transform: translateX(-50%) translateY(-50%);
}