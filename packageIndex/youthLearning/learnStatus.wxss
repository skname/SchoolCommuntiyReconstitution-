.head {
  width: 100%;
  height: 15vh;
  font-size: 35rpx;
  line-height: 15vh;
  border-radius: 0 0 20rpx 20rpx;
  text-align: center;
  color: #fff;
  background-color: #c3bef0;
}

.tip,
.tip .noSubmit,
.tip .submited {
  display: flex;
  flex-direction: row;
  align-items: center;
}

.tip {
  margin: 30rpx 0;
}

.submited {
  margin-left: 100rpx;
}

.noSubmit .square {
  box-sizing: border-box;
  border: 2rpx solid #ccc;
}

.submited .square {
  background-color: #aa96da;
}

.square {
  width: 40rpx;
  height: 40rpx;
  border-radius: 10rpx;
  margin-right: 10rpx;
}

.middle-content {
  width: 90%;
  display: flex;
  flex-direction: row;
  flex-wrap: wrap;
  align-content: flex-start;
}

.middle-item {
  margin: 5rpx .8%;
  width: 15%;
  height: 6vh;
  border-radius: 20rpx;
  box-sizing: border-box;
  color: rgb(182, 182, 182);
  border: 2rpx solid #ccc;
  display: flex;
  align-items: center;
  justify-content: center;
  font-size: 25rpx;
}

.submit {
  color: #fff;
  background-color: #aa96da;
}